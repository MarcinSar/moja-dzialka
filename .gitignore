data/ready-for-import/neo4j/*.gpkg
data/ready-for-import/neo4j/*.csv
data/ready-for-import/neo4j/csv/
data/ready-for-import/milvus/*.csv

# Zachowaj README i strukturę
!data/ready-for-import/README.md
!data/ready-for-import/.gitkeep
!data/ready-for-import/postgis/.gitkeep
!data/ready-for-import/neo4j/.gitkeep
!data/ready-for-import/milvus/.gitkeep

